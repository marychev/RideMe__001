2d_physics/layer_2="planks"
2d_physics/layer_3="rms"
2d_physics/layer_4="road"
2d_physics/layer_5="moving_panels"
2d_physics/layer_6="girl_back"
2d_physics/layer_7="kick_scooter"

[physics]

common/enable_pause_aware_picking=true
